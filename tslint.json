{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-security",
    "tslint-config-prettier"
  ],
  "jsRules": {  "max-line-length": {
    "options": [120]
}},
  "rules": {
    "object-literal-sort-keys": {
      "options": "match-declaration-order"
    },
    "prefer-template": true,
    "no-empty": false
  },
  "rulesDirectory": []
}
